<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.App.Starting" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/white</item> <!--Color background-->
        <item name="windowSplashScreenAnimatedIcon">@drawable/pngwing2</item>             <!--Image or gif splash-->
        <item name="windowSplashScreenAnimationDuration">1000</item>     <!--Duration splash-->
        <item name="postSplashScreenTheme">@style/Theme.DevelopersTools</item>  <!--Change theme at the end of the splash screen-->
        <item name="android:windowSplashScreenBrandingImage">@drawable/brand</item>
    </style>

</resources>